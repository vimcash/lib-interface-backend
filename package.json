{
  "name": "lib-interface-backend",
  "version": "1.0.0",
  "description": "Libreria de interfaces VimCash",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "build-node": "tsc src/index.ts --outDir dist",
    "dev": "ts-node-dev Pru.ts",
